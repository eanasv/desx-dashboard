{
    "compilerOptions": {
      "jsx": "react",
      "esModuleInterop": true,
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "module": "esnext",
      "moduleResolution": "node",
      "sourceMap": true,
      "allowJs": true,
      "strict": false,
      "noEmit": true,
      "isolatedModules": true,
      "resolveJsonModule": true,
      "noImplicitAny": false
    },
    "include": ["src"]
  }
  